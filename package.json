{
  "name": "codemancer",
  "version": "1.0.0",
  "description": "https://www.codemancer.com/",
  "directories": {
    "test": "tests"
  },
  "type": "module",
  "sideEffects": [
    "./codemancer/js/ganalytics.ts",
    "./codemancer/js/index.ts",
    "./codemancer/js/logfit.ts",
    "./codemancer/js/varsnap.ts",
    "./server/app.ts",
    "*.css"
  ],
  "scripts": {
    "start": "bin/start.sh",
    "eslint": "tsc --noEmit && eslint -c .eslint.config.ts .",
    "stylelint": "stylelint codemancer/css/*",
    "shellcheck": "git ls-files | grep -F .sh | xargs shellcheck --exclude=SC1091",
    "test": "pnpm run shellcheck && pnpm run stylelint && pnpm run test-node",
    "test-node": "pnpm run eslint && pnpm run build:dev && pnpm run wdio",
    "build:dev": "webpack-cli --mode=development",
    "build:prod": "webpack-cli --mode=production --node-env=production",
    "watch": "webpack-cli --watch",
    "wdio": "wdio run ./wdio.conf.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/albertyw/codemancer.git"
  },
  "author": "Albert Wang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/albertyw/codemancer/issues"
  },
  "homepage": "https://github.com/albertyw/codemancer#readme",
  "private": true,
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^3.4.2",
    "@types/dotenv-webpack": "^7.0.8",
    "@types/express": "^5.0.6",
    "@types/jquery": "^3.5.33",
    "@types/suncalc": "^1.9.2",
    "app-root-path": "^3.1.0",
    "axios": "^1.13.5",
    "core-js": "^3.47.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.4",
    "dotenv": "^17.2.3",
    "dotenv-webpack": "^8.1.1",
    "express": "^5.2.1",
    "jquery": "^3.7.1",
    "logfit": "^0.9.2",
    "mini-css-extract-plugin": "^2.9.4",
    "morgan": "^1.10.1",
    "mustache": "^4.2.0",
    "rollbar": "^2.26.5",
    "rotating-file-stream": "^3.2.7",
    "suncalc": "^1.9.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "varsnap": "^1.11.3",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-middleware": "^7.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.7.0",
    "@types/app-root-path": "^3.0.0",
    "@types/chai": "^5.2.3",
    "@types/mocha": "^10.0.10",
    "@types/morgan": "^1.9.10",
    "@types/mustache": "^4.2.6",
    "@types/node": "^25.0.9",
    "@types/sinon": "^21.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@wdio/browser-runner": "^9.23.0",
    "@wdio/cli": "^9.23.0",
    "@wdio/mocha-framework": "^9.23.0",
    "@wdio/spec-reporter": "^9.20.0",
    "chai": "^6.2.2",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "html-webpack-plugin": "^5.6.5",
    "mocha": "^11.7.5",
    "sinon": "^21.0.1",
    "stylelint": "^17.0.0",
    "stylelint-config-standard": "^40.0.0",
    "ts-mocha": "^11.1.0",
    "typescript-eslint": "^8.53.0"
  }
}
