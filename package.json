  },
  "homepage": "https://github.com/albertyw/codemancer#readme",
  "private": true,
  "dependencies": {
    "jquery": "^3.3.1",
    "moment": "^2.22.1",
    "moment-timezone": "^0.5.17",
    "q": "^1.5.1",
    "underscore": "^1.9.0"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "eslint": "^4.19.1",
    "mocha": "^3.2.0",
    "mocha-headless-chrome": "^2.0.0",
    "nyc": "^11.7.3",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0"
