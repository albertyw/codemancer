{
  "name": "codemancer",
  "version": "1.0.0",
  "description": "https://www.codemancer.com/",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "run": "bin/start.sh",
    "minify": "npm run build && npm run minify-css",
    "minify-css": "git ls-files codemancer/css | xargs node_modules/.bin/cleancss -o codemancer/css/codemancer.min.css -O2",
    "eslint": "tsc --noEmit && eslint . --ext .ts",
    "cover": "nyc --require dotenv/config --instrumenter false mochify --transform loose-envify --plugin tsify --require ts-node/register --require source-map-support/register --transform [ babelify --global --ignore [ test ] --presets [ @babel/preset-env ] --plugins [ babel-plugin-istanbul ] ] test/* --allow-chrome-as-root",
    "stylelint": "stylelint codemancer/css/*",
    "shellcheck": "git ls-files | grep -F .sh | xargs shellcheck --exclude=SC1091",
    "test": "npm run shellcheck && npm run test-node",
    "test-node": "npm run minify && npm run stylelint && npm run eslint && npm run cover",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/albertyw/codemancer.git"
  },
  "author": "Albert Wang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/albertyw/codemancer/issues"
  },
  "homepage": "https://github.com/albertyw/codemancer#readme",
  "private": true,
  "dependencies": {
    "@babel/preset-env": "^7.12.11",
    "@googlemaps/google-maps-services-js": "^3.3.16",
    "app-root-path": "^3.0.0",
    "axios": "^1.0.0",
    "babelify": "^10.0.0",
    "clean-css-cli": "^5.2.1",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "dotenv": "^16.0.0",
    "dotenv-webpack": "^8.0.1",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "logfit": "^0.8.2",
    "loose-envify": "^1.4.0",
    "mini-css-extract-plugin": "^2.7.5",
    "morgan": "^1.10.0",
    "mustache": "^4.1.0",
    "rollbar": "^2.19.4",
    "rotating-file-stream": "^3.0.2",
    "suncalc": "^1.9.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.0.0",
    "tsify": "^5.0.2",
    "typescript": "^5.0.0",
    "varsnap": "^1.6.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-middleware": "^6.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/app-root-path": "^1.2.4",
    "@types/chai": "^4.2.14",
    "@types/express": "^4.17.11",
    "@types/jquery": "^3.5.5",
    "@types/mocha": "^10.0.0",
    "@types/morgan": "^1.9.2",
    "@types/mustache": "^4.1.0",
    "@types/node": "^18.0.0",
    "@types/sinon": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "eslint": "^8.3.0",
    "html-webpack-plugin": "^5.5.0",
    "mocha": "^10.0.0",
    "mochify": "^9.0.0",
    "nyc": "^15.1.0",
    "sinon": "^15.0.0",
    "source-map-support": "^0.5.19",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "^33.0.0",
    "ts-mocha": "^10.0.0"
  }
}
