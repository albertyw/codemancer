{
  "name": "codemancer",
  "version": "1.0.0",
  "description": "https://www.codemancer.com/",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "eslint": "eslint codemancer/js test",
    "istanbul-instrument": "istanbul instrument -i ./codemancer/js -o ./instrumented",
